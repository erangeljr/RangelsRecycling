<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Rangel's Recycling</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <!--Head Section start-->
    <header>
        <div class="container">
            <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12">
                <div class="eco_logo">
                    <a href="index.html">
                        <img src="~/images/ecorecycling-logo-main.png" class="img-responsive" alt="">
                    </a>
                </div>
            </div>
            <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12  pull-right">
                <div class="eco_info">
                    <div class="eco_ph padding_lr"><i class="fa fa-phone"></i>(661) 865-8070</div>
                    <div class="eco_email"><i class="fa fa-envelope"></i>info@rangelsrecycling.com</div>
                </div>
                <div class="eco_social_icon">
                    <ul>
                        <li>
                            <a href="https://www.facebook.com/rangelsrecycling" title="Facebook"><i class="fa fa-facebook"></i></a>
                        </li>
                        <li>
                            <a href="#" title="Twitter">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Change Me">
                                <i class="fa fa-dribbble"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
    <!--Head Section End-->
    <!--Menu Start-->
    <div class="eco_header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 padding_lr">
                    <nav class="navbar navbar-inverse" role="navigation">
                        <div class="navbar-header">
                            <button type="button" id="nav-toggle" class="navbar-toggle" data-toggle="collapse" data-target="#main-nav"><span class="eco_toggle_menu"><span class="glyphicon glyphicon-list"></span></span><span class="eco_toggle_text">MENU</span> <span class="sr-only">Toggle navigation</span> </button>
                        </div>
                        <!--navbar-header-->
                        <div id="main-nav" class="collapse navbar-collapse padding_left">
                            <ul class="nav navbar-nav my_menu">
                                <li class="eco_pos_rel">@Html.ActionLink("Home", "Index", "Home")</li>
                                <li class="eco_pos_rel">@Html.ActionLink("About", "About", "Home")</li>
                                <li class="eco_pos_rel">@Html.ActionLink("Contact", "Contact", "Home")</li>
                                <li class="eco_pos_rel eco_search">
                                    <a><i class="fa fa-search"></i></a>
                                    <div class="eco_search_form">
                                        <form>
                                            <input type="text" name="name" class="eco_input" placeholder="search...">
                                        </form>
                                    </div>
                                </li>
                            </ul>
                            @Html.Partial("_LoginPartial")
                        </div>
                        <!--navbar-collapse-->
                    </nav>
                </div>
            </div>
            <!--navbar-->
        </div>
        <!--container-->
    </div>
    <!--header-->
    <!--second menu start-->
    <div class="pagenav fixedmenu">
        <div class="holder-fixedmenu">
            <div class="logo-fixedmenu">
                <a href="index.html">
                    <img src="images/ecorecycling-logo-main.png" alt="">
                </a>
            </div>
            <div class="menu-fixedmenu home">
                <ul id="menu-home-page" class="menu my_menu">
                    <li class="eco_pos_rel">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li class="eco_pos_rel">@Html.ActionLink("About", "About", "Home")</li>
                    <li class="eco_pos_rel">@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li class="eco_pos_rel eco_search1">
                        <a><i class="fa fa-search"></i></a>
                        <div class="eco_search_form1">
                            <form>
                                <input type="text" name="name" class="eco_input" placeholder="Search...">
                            </form>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!--second menu end-->
    <!--Menu Section End-->

    @RenderBody()
    <hr />
    <footer>
        <div class="eco_copyight">
            Rangel's Recycling &copy; @DateTime.Now.Year by <a href="http://www.rangelsrecycling.com/" class="eco_color5">Rangel's Recycling, LLC</a>
        </div>
    </footer>

<script src="~/Scripts/jquery-1.11.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.bxslider.min.js"></script>
<script src="~/Scripts/plugins/prettyphoto/jquery.prettyPhoto.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/rainyday/rainyday.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.mixitup.js" type="text/javascript"></script>
<script src="~/Scripts/scrollReveal.js" type="text/javascript"></script>
<script src="~/Scripts/circles.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/countto/jquery.countTo.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/countto/jquery.appear.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/parallax/jquery.parallax-1.1.3.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/revolution/js/jquery.themepunch.revolution.min.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/revolution/js/jquery.themepunch.tools.min.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/jquery-ui/jquery-ui.js" type="text/javascript"></script>
<script src="~/Scripts/plugins/smoothscroll/smoothScroll.js" type="text/javascript"></script>
<script src="~/Scripts/custom.js" type="text/javascript"></script>
@RenderSection("scripts", required: false)
</body>
</html>
